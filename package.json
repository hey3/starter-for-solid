{
  "name": "starter-for-solid",
  "version": "1.0.0",
  "description": "My boilerplate for Web Frontend using SolidJS.",
  "keywords": [
    "solidjs",
    "typescript",
    "vite",
    "tailwindcss",
    "pnpm",
    "boilerplate-template"
  ],
  "homepage": "https://github.com/hey3/starter-for-solid#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-solid/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-solid.git"
  },
  "engines": {
    "node": "18.x"
  },
  "packageManager": "pnpm@8.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write--ignore-unknown",
    "lint": "pnpm lint:eslint && pnpm lint:style",
    "lint:fix": "pnpm lint:eslint-fix && pnpm lint:style-fix",
    "lint:eslint": "eslint . --ext .js,.ts,.tsx",
    "lint:eslint-fix": "eslint . --fix --ext .js,.ts,.tsx",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:style-fix": "stylelint --fix 'src/**/*.css'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:virtual-update": "vitest run --update",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "solid-app-router": "0.4.2",
    "solid-js": "1.7.3"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.0.4",
    "@storybook/addon-essentials": "7.0.4",
    "@storybook/html": "7.0.4",
    "@storybook/html-vite": "7.0.4",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-solid": "0.12.0",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-unused-imports": "2.0.0",
    "jsdom": "21.1.1",
    "prettier": "2.8.7",
    "storybook": "7.0.4",
    "stylelint": "15.4.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-standard": "32.0.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vite-plugin-solid": "2.7.0",
    "vitest": "0.29.8"
  }
}
