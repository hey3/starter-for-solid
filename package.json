{
  "name": "starter-for-solid",
  "version": "1.0.0",
  "description": "My boilerplate for Web Frontend using SolidJS.",
  "keywords": [
    "solidjs",
    "typescript",
    "vite",
    "tailwindcss",
    "pnpm",
    "boilerplate-template"
  ],
  "homepage": "https://github.com/hey3/starter-for-solid#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-solid/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-solid.git"
  },
  "packageManager": "pnpm@7.1.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --check './src/**/*.{ts,tsx,json}'",
    "format:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:virtual-update": "vitest run --update",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "solid-app-router": "0.3.3",
    "solid-js": "1.4.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.17.12",
    "@storybook/addon-a11y": "6.5.3",
    "@storybook/addon-essentials": "6.5.3",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/html": "6.5.3",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "autoprefixer": "10.4.7",
    "babel-preset-solid": "1.4.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-solid": "0.4.7",
    "eslint-plugin-tailwindcss": "3.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jsdom": "19.0.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "vite": "2.9.9",
    "vite-plugin-solid": "2.2.6",
    "vitest": "0.12.8"
  }
}
