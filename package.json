{
  "name": "starter-for-solid",
  "version": "1.0.0",
  "description": "My boilerplate for Web Frontend using SolidJS.",
  "keywords": [
    "solidjs",
    "typescript",
    "vite",
    "tailwindcss",
    "pnpm",
    "boilerplate-template"
  ],
  "homepage": "https://github.com/hey3/starter-for-solid#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-solid/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-solid.git"
  },
  "packageManager": "pnpm@7.13.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write--ignore-unknown",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:virtual-update": "vitest run --update",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "solid-app-router": "0.4.2",
    "solid-js": "1.5.7"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/html": "6.5.12",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-solid": "0.7.3",
    "eslint-plugin-tailwindcss": "3.6.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "jsdom": "20.0.1",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-plugin-solid": "2.3.9",
    "vitest": "0.23.4"
  }
}
